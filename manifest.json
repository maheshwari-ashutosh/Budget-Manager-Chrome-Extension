{
  "manifest_version": 2,
  "name": "Budget App",
  "version": "0.0.1",
  "description": "An app to track budget",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "options_page": " options.html",
  "permissions":[
    "storage",
    "notifications"
  ]
}